{
  "name": "quakejs",
  "version": "0.0.1",
  "scripts": {
    "start": "node bin/web.js --config ./web.json",
    "start:content": "node bin/content.js",
    "build": "make PLATFORM=js USE_LOCAL_HEADERS=1",
    "build:assets": "node bin/repak.js --src ../../data --dest ./assets"
  },
  "bin": {
    "quakejs-content": "bin/content.js",
    "quakejs-master": "bin/master.js",
    "quakejs-repak": "bin/repak.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/inolen/quakejs.git"
  },
  "author": "Anthony Pesch",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "async": "~0.2.9",
    "buffer-crc32": "~0.2.1",
    "ejs": "~0.8.4",
    "express": "~3.3.5",
    "optimist": "~0.6.0",
    "quakejs-files": "0.0.3",
    "send": "^0.16.2",
    "temp": "~0.5.1",
    "underscore": "^1.5.2",
    "winston": "2.4.0",
    "wrench": "~1.5.4",
    "ws": "~0.4.29"
  }
}
